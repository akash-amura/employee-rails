<b>Address</b>
<%= fields_for address_sym.to_sym,address do |f| %>
  <% if edit %>
    <p>
    <%= f.label :id %>
    <%= f.text_field :id %>
    </p>
  <% end %>
  <p> 
  <%=  f.label :building_number %>
  <%=  f.text_field :building_number %>
  </p>  

  <p> 
  <%=  f.label :street_name %>
  <%=  f.text_field :street_name %>
  </p>  


  <p> 
  <%=  f.label :street_address %>
  <%=  f.text_field :street_address %>
  </p>  

  <p> 
  <%=  f.label :secondary_address %>
  <%=  f.text_field :secondary_address %>
  </p>  

  <p> 
  <%=  f.label :city %>
  <%=  f.text_field :city %>
  </p>  

  <p> 
  <%=  f.label :zip_code %>
  <%=  f.text_field :zip_code %>
  </p>  

  <p> 
  <%=  f.label :state %>
  <%=  f.text_field :state %>
  </p>  

  <p> 
  <%=  f.label :country %>
  <%=  f.text_field :country %>
  </p>  

  <p> 
  <%=  f.label :longitude %>
  <%=  f.text_field :longitude %>
  </p>  

  <p> 
  <%=  f.label :latitude %>
  <%=  f.text_field :latitude %>
  </p>  

<%  end %>
